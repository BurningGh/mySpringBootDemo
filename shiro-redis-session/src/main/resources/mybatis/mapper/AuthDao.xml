<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.shiroredissession.dao.AuthDao">

    <select id="queryAuthList" resultType="Auth">
        select * from auth
    </select>

    <delete id="deleteOneAuthById">
        delete from auth where id = #{id}
    </delete>

</mapper>